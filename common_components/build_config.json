{
    "bower": {
        "name": "{{name}}",
        "dependencies": {}
    },

    "common_components": [
        "{{common_components}}LanguageController",
        "{{common_components}}HeaderController/",
        "{{common_components}}Extensions/",
        "{{common_components}}Utils/"
    ],

    "local_components": [
    ],

    "bower_components": [
        "jsurl"
    ],

    "remote_sources": {
        "js": [{
            "http://maps.kosmosnimki.ru/api/": [{
                    "path": "api.js?key={{api_key}}",
                    "encoding": "windows-1251"
                },
                "layersTree.js",
                "drawingObjects.js",
                "utilities.js"
            ],
            "http://search.kosmosnimki.ru/": [
                "client/js/GeoSearchController.js"
            ]
        }]
    },

    "local_sources": {
        "js": [{
            "client/common_components/": [
                "LanguageController/build/languageController.js",
                "HeaderController/build/headerController.js",
                "Extensions/build/extensions.js",
                "Utils/build/utils.js"
            ]
        }, {
            "client/bower_components/": [
                "jsurl/url.js"
            ]
        }],
        "css": [{
            "client/common_components/": [
                "LanguageController/build/assets/styles.css",
                "HeaderController/build/assets/styles.css"
            ]
        }, 
        "adapter.css"]
    }
}